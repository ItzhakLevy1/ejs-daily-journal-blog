<%- include("partials/header"); -%>
  <h1>Compose</h1>
  <form action="/compose" method="post">
    <div class="form-group">
    <label for="" class="form-label">Title</label>
    <input class="form-control" name="postTitle" type="text" id="postTitle">
    <label for="textArea">Post</label>
    <textarea class="form-control" name="postBody" rows="8" cols="80"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" id="composeBtn">Publish</button>
  </form>
<%- include("partials/footer"); -%>
